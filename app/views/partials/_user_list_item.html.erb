  <li class="card list-group-item">
    <div class="user-container">
      <p><%= user.id %></p>
      <p>
        <%= link_to user.first_name + " " + user.last_name, user_path(user.id) %>
      </p>
      <p><%= user.email %></p>
      <p><%= user.role %></p>
      <p><%= user.created_at %></p>
    </div>
  </li>